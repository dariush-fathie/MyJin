<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="end"
    android:background="#fff"
    android:clickable="true"
    android:focusable="true">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/viewx">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <android.support.v7.widget.AppCompatImageView
                android:id="@+id/iv_jinDrawer"
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/menu_title" />

            <RelativeLayout
                android:id="@+id/rl_login_outsign"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginBottom="4dp">


                <View
                    android:id="@+id/vs2"
                    android:layout_width="4dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:background="@color/green"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/ivs2"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="8dp"
                    android:layout_toStartOf="@+id/vs2"
                    android:padding="6dp"
                    android:src="@drawable/ic_password"
                    android:tint="@color/green" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_login_outsign"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toStartOf="@+id/ivs2"
                    android:gravity="center_vertical"
                    android:paddingEnd="17dp"
                    android:text="ورود / عضویت"
                    android:textColor="@color/title"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:fontFamily="@font/yekan" />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginBottom="7dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="20dp"
                android:background="#333" />

            <RelativeLayout

                android:id="@+id/rl_myjin_services"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginBottom="4dp">


                <View
                    android:id="@+id/v1"
                    android:layout_width="4dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:background="@color/green"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/iv1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="8dp"
                    android:layout_toStartOf="@+id/v1"
                    android:padding="6dp"
                    android:src="@drawable/ic_jin"
                    android:tint="@color/green" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_myjin_services_Title1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toStartOf="@+id/iv1"
                    android:gravity="center_vertical"
                    android:paddingEnd="17dp"
                    android:text="خدمات ژین من"
                    android:textSize="17sp"
                    android:textColor="@color/green"
                    app:fontFamily="@font/yekan" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginBottom="7dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="20dp"
                android:background="@color/split" />


            <RelativeLayout
                android:id="@+id/rl_takapoo_services"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginBottom="7dp">
                <View
                    android:id="@+id/vs3"
                    android:layout_width="4dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:background="@color/green"
                    android:visibility="invisible" />

                <ImageView
                    android:id="@+id/ivs3"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="8dp"
                    android:layout_toStartOf="@+id/vs3"
                    android:padding="5dp"
                    android:src="@drawable/ic_takapo"
                    android:tint="@color/green" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/tv_takapoo_services_Title1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toStartOf="@+id/ivs3"
                    android:gravity="center_vertical"
                    android:paddingEnd="17dp"
                    android:textColor="@color/subTitle"
                    android:text="خدمات مرکز نوآوری و فناوری سلامت(تکاپو)"
                    android:textSize="16sp"
                    app:fontFamily="@font/yekan" />
            </RelativeLayout>
            <View
